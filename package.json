{
  "name": "@testament/core-service",
  "version": "0.2.1",
  "description": "Testament Core Service",
  "author": "jlazarre95 <jlazarre95@gmail.com>",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/testament-team/testament-core-service.git"
  },
  "files": [
    "dist/src/**",
    "index.js",
    "index.d.ts",
    "index.js.map"
  ],
  "keywords": [
    "testament",
    "core",
    "service"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:prod": "rimraf dist && ttsc -p tsconfig.build.json",
    "build:watch": "nest build --watch",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "mocha --config test/mocharc.unit.yml",
    "test:debug": "mocha --inspect-brk",
    "test:cov": "nyc mocha",
    "pretest:int": "docker-compose -f docker-compose.int.yml down -v --remove-orphans && docker-compose -f docker-compose.int.yml build && docker-compose -f docker-compose.int.yml up -d && echo \"Waiting 60 seconds for dependencies to start...\" && sleep 60",
    "test:int": "mocha --config test/mocharc.int.yml",
    "test:int:only": "mocha --config test/mocharc.int.yml",
    "posttest:int": "docker-compose -f docker-compose.int.yml down -v --remove-orphans",
    "test:int:logs": "mkdir -p tmp && docker-compose -f docker-compose.int.yml logs --no-color > tmp/integration-test.log",
    "pretest:all": "npm run pretest:int",
    "test:all": "mocha --config test/mocharc.all.yml",
    "posttest:all": "npm run posttest:int",
    "pretest:all:cov": "npm run pretest:all",
    "test:all:cov": "nyc mocha --config test/mocharc.all.yml",
    "posttest:all:cov": "npm run posttest:all",
    "open:cov": "open coverage/index.html",
    "docker:build": "docker build -t testament-core-service .",
    "docker:run": "docker run --rm -p 8080:8080 --name testament-core-service testament-core-service",
    "docker:buildAndRun": "npm run docker:build && npm run docker:run",
    "docker:run:watch": "tsc-watch --onSuccess \"npm run docker:buildAndRun\"",
    "docker:stop": "docker rm -f testament-core-service",
    "docker:compose:up": "docker-compose up",
    "docker:compose:down": "docker-compose down -v --remove-orphans && docker-compose rm -f -s",
    "docker:compose:up:all": "docker-compose -f docker.compose.all.yml up",
    "docker:compose:down:all": "docker-compose -f docker.compose.all.yml down",
    "prepare": "npm run build:prod",
    "prepublishOnly": "npm test",
    "version": "git add -A",
    "postversion": "git push && git push --tags"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": false,
    "require": [
      "ts-node/register/transpile-only"
    ],
    "exclude": [
      "**/*.d.ts",
      "**/*.spec.ts",
      "**/main.ts",
      "coverage",
      "bin",
      "**/*.interface.ts",
      "**/util/test"
    ],
    "reporter": [
      "text",
      "html"
    ]
  },
  "dependencies": {
    "@nestjs/common": "^6.7.2",
    "@nestjs/core": "^6.7.2",
    "@nestjs/mongoose": "^6.1.2",
    "@nestjs/platform-express": "^6.7.2",
    "@nestjs/swagger": "^4.6.0",
    "@typegoose/typegoose": "^7.3.1",
    "amqplib": "^0.6.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.14.0",
    "fs-extra": "^8.1.0",
    "mongoose": "^5.9.28",
    "nestjs-typegoose": "^7.1.32",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.8",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.3",
    "swagger-ui-express": "^4.1.4"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@nestjs/cli": "^7.1.5",
    "@nestjs/schematics": "^6.7.0",
    "@nestjs/testing": "^6.7.1",
    "@types/amqplib": "^0.5.13",
    "@types/chai": "^4.2.10",
    "@types/chai-as-promised": "^7.1.2",
    "@types/express": "^4.17.1",
    "@types/fs-extra": "^8.0.1",
    "@types/mongoose": "^5.7.36",
    "@types/node": "^12.7.5",
    "@types/request": "^2.48.4",
    "@types/request-promise-native": "^1.0.17",
    "@types/supertest": "^2.0.8",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "mocha": "^7.1.2",
    "mocha-typescript": "^1.1.17",
    "nyc": "^15.0.0",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2",
    "ts-mockito": "^2.5.0",
    "ts-node": "^8.4.1",
    "tsc-watch": "^4.2.9",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.0",
    "ttypescript": "^1.5.7",
    "typescript": "^3.9.3",
    "typescript-transform-paths": "^1.1.11"
  }
}
